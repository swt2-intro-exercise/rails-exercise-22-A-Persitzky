<h1>Listing Articles</h1>

<table>
    <tr>
        <th>Name</th>
        <th>Homepage</th>
        <th></th>
    </tr>

    <% @authors.each do |author| %>
        <tr>
            <td><%= author.name %></td>
            <td><a href="<%= author.homepage %>"><%= author.homepage %></a></td>
            <td><%= link_to 'Show', author_path(author) %></td>
        </tr>
    <% end %>
</table>
<%= link_to 'New', new_author_path %>